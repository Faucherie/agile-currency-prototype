<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Currency Exchange Rates</title>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCRSFexpTRDtt_QrFno8URDmDoGSSrdLGQ"></script>
</head>
<body>
    <h1>Currency Exchange Rates</h1>
    <table border="1">
        <tr>
            <th>Bank</th>
            <th>Exchange Rate</th>
        </tr>
        <% rates.forEach(rate => { %>
            <tr>
                <td><%= rate.bank %></td>
                <td><%= rate.rate %></td>
            </tr>
        <% }) %>
    </table>
    <div id="map" style="height: 400px; width: 100%;"></div>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 40.4093, lng: 49.8671 }, // Baku coordinates
                zoom: 12,
            });
        });
    </script>
</body>
</html>
